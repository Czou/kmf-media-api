<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="mediaManagerListener" class="com.kurento.kms.media.TestMediaManagerListener" />
	<bean id="mediaManagerFactory" class="com.kurento.kms.media.MediaManagerFactory">
		<constructor-arg value="localhost" />
		<constructor-arg value="9090" />
		<constructor-arg ref="mediaManagerListener" />
	</bean>
</beans>