<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="mediaManagerHandler" class="com.kurento.kms.media.TestMediaManagerHandler" />
	<bean id="mediaManagerFactory" class="com.kurento.kms.media.MediaManagerFactory">
		<constructor-arg value="localhost" />
		<constructor-arg value="9090" />
		<constructor-arg ref="mediaManagerHandler" />
		<constructor-arg value="0" /> <!--TODO: use a correct handlerId -->
		<constructor-arg value="localhost" />
		<constructor-arg value="9191" />
	</bean>
</beans>
